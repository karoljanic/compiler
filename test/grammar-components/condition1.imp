PROGRAM IS
    a, b, c[10], d[10]
IN
    a := 0;
    b := a;

    IF 0 = 0 THEN
        WRITE 1;
    ELSE
        WRITE 0;
    ENDIF

    IF a = b THEN
        WRITE 1;
    ENDIF

    IF c[0] = d[0] THEN
        WRITE 1;
    ELSE
        WRITE 0;
    ENDIF

    IF c[b] = d[a] THEN
        WRITE 1;
    ENDIF

    IF c[0] = d[c[0]] THEN
        WRITE 1;
    ELSE
        WRITE 0;
    ENDIF
END