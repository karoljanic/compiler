PROGRAM IS
    x[15], a, b, c, d[10]
IN
    a := 1;
    b := 3;

    c :=  7 - 2;
    c := 8 - a;
    c := b - 9;
    c := 3 - d[1];
    c := d[2] - 4;
    c := 4 - d[a];
    c := d[b] - 5;

    c := a - b;
    c := a - d[1];
    c := d[1] - a;
    c := a - d[b];
    c := d[b] - a;

    c := d[1] - d[2];
    c := d[1] - d[a];
    c := d[b] - d[2];

    c := d[a] - d[b];
END