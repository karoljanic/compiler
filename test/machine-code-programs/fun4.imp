PROCEDURE write(x) IS

IN
  WRITE x;
END

PROGRAM IS
  a,b,c,d
IN
    READ a;
    READ b;
    READ c;
    READ d;

    WHILE a < 4 DO
      write(a);
      a := a + 1;
    ENDWHILE

    REPEAT
      write(b);
      b := b + 1;
    UNTIL b > 5;

    IF c < 10 THEN
      write(c);
    ELSE
      write(d);
    ENDIF

    IF d = 2 THEN
        IF d < 10 THEN
          write(d);
        ENDIF
        write(d);
    ENDIF
END
